\section{Matheumgebung} {\label{math}}
\begin{enumerate}
    \item[1)] $a^2+b^2=c^2$
    \item[2)] $(a+b)\cdot(a+b) = a^2+b^2$
    \item[3)] $\frac{a}{b} \div \frac{c}{d} = \frac{a}{b}\cdot\frac{c}{d}$
    \item[4)] $f(x) = \ln(\frac{\frac{x}{2}}{\pi}) + \sin(x)$
    \item[5)] $f_a(x) = e^{\frac{x^2}{x + 2} + 2x} + ax$
    \item[6)] $g^\prime(x) = 2x$
    \item[7)] $\int_a^bf(x)dx$
    \item[8)] $(\sum_{i=1}^nf_i(x))^\prime$
    \item[9)] $(a -b)(a + b) = a^2 - b^2$
\end{enumerate}


\section{pq-formel}{\label{pq}}
$&
   0 = x^2+10x+16 \\
=> 0 = x^2 + px + q \\
=> 0 = x^2 + px + (\frac{P}{2})^2 - (\frac{P}{2})^2 + q \\
=> 0 = (x +\frac{p}{2})^2-(\frac{P}{2})^2 + q \\
=> (\frac{p}{2})^2 = (x + \frac{p}{2})^2  |  \sqrt{...} \\
=> \pm\sqrt{(\frac{p}{2})^2-q} = x + \frac{p}{2} | -\frac{p}{2} \\
=> x_1 = -\frac{p}{2} + \sqrt{(\frac{p}{2})^2-q} \\
=> x_2 = -\frac{p}{2} - \sqrt{(\frac{p}{2})^2-q} $\begin{proof}\\
